<!DOCTYPE html>

<html class="no-js">
  <head itemscope itemtype="http://schema.org/Website">
    <meta charset="utf-8" />

    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <meta http-equiv="Refresh" content="180" />

    #if $Extras.has_key('cwop')

    <meta
      name="description"
      content="$station.location Weather Station ($Extras.cwop) - weather web site powered by WeeWX"
    />

    #else

    <meta
      name="description"
      content="$station.location Weather Station - weather web site powered by WeeWX"
    />

    #end if

    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="profile" href="http://gmpg.org/xfn/11" />

    #if $Extras.has_key('web_url')

    <link href="$Extras.web_url/week.html" itemprop="url" />

    #end if
    <title itemprop="name">$station.location Weekly Weather Summary</title>

    <!-- Open Graph -->

    <meta
      property="og:site_name"
      content="$station.location Sommario settimanale"
    />

    <meta property="og:title" content="Sommario settimanale" />

    #if $Extras.has_key('web_url')

    <meta property="og:url" content="$Extras.web_url/week.html" />

    #end if

    <meta
      property="og:description"
      content="Sommario meteo settimanale di $station.location - weather web site powered by WeeWX"
    />

    <meta property="og:type" content="website" />

    <!-- Icons -->

    <link rel="icon" href="favicon.ico" type="image/png" />

    <link rel="apple-touch-icon" href="touch-icon.png" />

    <!-- Styles -->

    <link
      href="http://fonts.googleapis.com/css?family=Lato:300,400,700"
      rel="stylesheet"
      type="text/css"
    />

    <link
      href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
      rel="stylesheet"
    />

    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
      integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />

    <!-- <link rel="stylesheet" href="css/main.css" /> -->

    <link rel="stylesheet" href="build.css?2" />

    <!-- Skripte -->

    <script src="js/modernizr-2.6.2.min.js"></script>

    <script src="js/checkdiff.js"></script>

    <!-- Umami -->

    <script
      async
      src="https://analytics.umami.is/script.js"
      data-website-id="2e66cfde-2766-46b3-9883-193bcb330af9"
    ></script>
  </head>

  <body>
    <!-- Header -->
    <div id="header" class="tw:px-8 tw:pt-6 tw:bg-weather-cool/80">
      <div
        class="tw:lg:max-w-[75%] tw:mx-auto tw:text-left tw:relative tw:z-10 tw:text-white"
      >
        <div class="tw:flex tw:items-center tw:gap-3 tw:mb-2">
          <a href="/">
            <h1 class="tw:text-4xl tw:font-semibold tw:tracking-tight">
              $station.location
            </h1>
          </a>
        </div>

        <div
          class="tw:flex tw:items-center tw:gap-5 tw:mb-2 tw:text-sm tw:text-white/85 tw:font-medium"
        >
          <div class="tw:flex tw:items-center tw:gap-1.5">
            <svg
              class="tw:w-3.5 tw:h-3.5 tw:opacity-70"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
            >
              <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z" />
              <circle cx="12" cy="10" r="3" />
            </svg>
            <span class="tw:font-mono"
              >$station.latitude[0]°$station.latitude[1]'$station.latitude[2],
              $station.longitude[0]°$station.longitude[1]'$station.longitude[2]</span
            >
          </div>
          <div class="tw:flex tw:items-center tw:gap-1.5">
            <svg
              class="tw:w-3.5 tw:h-3.5 tw:opacity-70"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
            >
              <polyline points="17,10 12,15 7,10" />
              <polyline points="19,15 12,22 5,15" />
            </svg>
            <span class="tw:font-mono">$station.altitude</span>
          </div>
        </div>

        <p class="tw:text-base tw:text-white/90 tw:font-medium tw:mb-4">
          Ultimo aggiornamento:
          <span class="tw:font-mono"
            >$current.dateTime $current.dateTime.format("%Z")</span
          >
        </p>

        <div
          class="tw:flex tw:items-end tw:justify-between tw:flex-wrap tw:gap-4"
        >
          <!-- PC menu -->
          <nav class="tw:flex-1">
            <div class="tw:hidden tw:md:flex tw:flex-row tw:gap-1">
              <a
                href="index.html"
                class="tw:px-3 tw:py-2 tw:text-white/80 tw:text-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-t-md"
                >Oggi</a
              >
              <a
                href="week.html"
                class="tw:px-3 tw:py-2 tw:text-white tw:text-lg tw:font-medium tw:border-b-2 tw:border-white/80 tw:transition-colors hover:tw:bg-white/10 tw:rounded-t-md"
                >Settimana</a
              >
              <a
                href="month.html"
                class="tw:px-3 tw:py-2 tw:text-white/80 tw:text-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-t-md"
                >Mese</a
              >
              <a
                href="year.html"
                class="tw:px-3 tw:py-2 tw:text-white/80 tw:text-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-t-md"
                >Anno</a
              >
              <a
                href="summary.html"
                class="tw:px-3 tw:py-2 tw:text-white/80 tw:text-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-t-md"
                >Storico</a
              >
              <a
                href="vaia.html"
                class="tw:px-3 tw:py-2 tw:text-white/80 tw:text-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-t-md"
                >Vaia</a
              >
            </div>
            <!-- Mobile menu button -->
            <div class="tw:md:hidden">
              <button
                class="tw:outline-none mobile-menu-button tw:p-2 tw:rounded-lg hover:tw:bg-white/10 tw:transition-colors"
              >
                <svg
                  class="tw:w-6 tw:h-6 tw:text-white"
                  x-show="!showMenu"
                  fill="none"
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  viewBox="0 0 24 24"
                  stroke="currentColor"
                >
                  <path d="M4 6h16M4 12h16M4 18h16"></path>
                </svg>
              </button>
            </div>
          </nav>

          <!-- IG e aduepassi -->
          <div class="tw:flex tw:items-center tw:gap-3">
            <a
              href="https://www.instagram.com/meteocentrocadore/?hl=it"
              class="tw:p-2 tw:text-white/80 tw:text-xl tw:transition-colors hover:tw:text-white hover:tw:bg-white/10 tw:rounded-lg"
              ><i class="fab fa-instagram"></i
            ></a>
            <a
              data-umami-event="A due passi"
              href="https://aduepassidallaciclabile.it"
              class="tw:bg-lime-400 tw:text-gray-900 tw:rounded-full tw:px-4 tw:py-2 tw:text-sm tw:font-medium tw:transition-all hover:tw:bg-lime-300 hover:tw:shadow-lg tw:shadow-lime-500/25"
            >
              <i class="fa-brands fa-airbnb tw:mr-1.5"></i> A due passi dalla
              ciclabile
            </a>
          </div>
        </div>

        <!-- Mobile menu -->
        <div
          class="tw:hidden mobile-menu tw:mt-4 tw:pt-4 tw:border-t tw:border-white/20"
        >
          <ul class="tw:space-y-1">
            <li>
              <a
                href="index.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white/80 tw:text-lg tw:rounded-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10"
                >Oggi</a
              >
            </li>
            <li>
              <a
                href="week.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white tw:text-lg tw:bg-white/10 tw:rounded-lg tw:font-medium"
                >Settimana</a
              >
            </li>
            <li>
              <a
                href="month.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white/80 tw:text-lg tw:rounded-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10"
                >Mese</a
              >
            </li>
            <li>
              <a
                href="year.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white/80 tw:text-lg tw:rounded-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10"
                >Anno</a
              >
            </li>
            <li>
              <a
                href="summary.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white/80 tw:text-lg tw:rounded-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10"
                >Storico</a
              >
            </li>
            <li>
              <a
                href="vaia.html"
                class="tw:block tw:px-3 tw:py-3 tw:text-white/80 tw:text-lg tw:rounded-lg tw:transition-colors hover:tw:text-white hover:tw:bg-white/10"
                >Vaia</a
              >
            </li>
          </ul>
        </div>
      </div>
    </div>
    <!-- End header -->

    <main>
      <div
        class="tw:container tw:mx-auto tw:grid tw:grid-cols-1 tw:lg:grid-cols-5 tw:gap-6 tw:py-4"
      >
        <div class="tw:lg:col-span-2">
          <div class="tw:bg-weather-cool/6 tw:rounded-2xl">
            <h1 class="tw:text-xl tw:text-gray-800 tw:px-4 tw:pt-4">
              Questa settimana ($week.start.format("%d-%m-%Y") -
              $week.end.format("%d-%m-%Y"))
            </h1>

            <table class="tw:w-full tw:table-fixed tw:border-collapse tw:mt-2">
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Temperatura massima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outTemp.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outTemp.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Temperatura minima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outTemp.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outTemp.mintime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Punto di rugiada massimo:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.dewpoint.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.dewpoint.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Punto di rugiada minimo:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.dewpoint.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.dewpoint.mintime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Umidità massima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outHumidity.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outHumidity.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Umidità minima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outHumidity.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.outHumidity.mintime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Pressione massima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.barometer.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.barometer.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Pressione minima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.barometer.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.barometer.mintime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Pioggia totale:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.rain.sum
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                ></td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Velocità massima del vento:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.wind.max $week.wind.gustdir
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.wind.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Velocità media del vento:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.wind.avg
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                ></td>
              </tr>
              #if $week.extraTemp1.has_data
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Massima Pond Temp:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.extraTemp1.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.extraTemp1.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Minima Pond Temp:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.extraTemp1.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.extraTemp1.mintime
                </td>
              </tr>
              <!-- prettier-ignore -->
              #end if 
              #if $week.UV.has_data
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Massima UV:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.UV.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.UV.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Minima UV:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.UV.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.UV.mintime
                </td>
              </tr>
              <!-- prettier-ignore -->
              #end if 
              #if $week.ET.has_data and $week.ET.sum.raw > 0.0
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Massima ET:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.ET.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.ET.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Minima ET:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.ET.min
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.ET.mintime
                </td>
              </tr>
              <!-- prettier-ignore -->
              #end if 
              #if $week.radiation.has_data
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Massima Radiation:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.radiation.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $week.radiation.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10 tw:group">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:pl-4 tw:group-hover:rounded-bl-2xl"
                >
                  Minima Radiation:
                </td>
                <td class="tw:py-2 tw:text-gray-900">$week.radiation.min</td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:group-hover:rounded-br-2xl"
                >
                  $week.radiation.mintime
                </td>
              </tr>
              #end if
            </table>
          </div>

          <div class="tw:bg-weather-cool/6 tw:mt-4 tw:rounded-2xl">
            <h1 class="tw:text-xl tw:text-gray-800 tw:px-4 tw:pt-4">
              Ieri ($yesterday.dateTime.format("%d-%b-%Y"))
            </h1>

            <table class="tw:w-full tw:table-fixed tw:border-collapse tw:mt-2">
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Temperatura massima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $yesterday.outTemp.max
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $yesterday.outTemp.maxtime
                </td>
              </tr>
              <tr class="tw:hover:bg-weather-cool/10">
                <td
                  class="tw:pr-10 tw:py-2 tw:font-medium tw:text-gray-900 tw:border-b tw:border-gray-200 tw:pl-4"
                >
                  Temperatura minima:
                </td>
                <td
                  class="tw:py-2 tw:text-gray-900 tw:border-b tw:border-gray-200"
                >
                  $yesterday.outTemp.min
                </td>
                <td
